<?xml version="1.0" encoding="utf-8"?>


<TableRow xmlns:tools="http://schemas.android.com/tools"
    style="@style/Base.Widget.AppCompat.SeekBar.Discrete"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    >

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_span="3">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/Name" >

            <TextView
                android:text="TextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/nameR"
                android:textStyle="normal|bold"
                android:textSize="20sp"
                android:layout_span="3"
                android:textColor="@color/cast_libraries_material_featurehighlight_text_body_color" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/Objects" >

            <TextView
                android:text="Website"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/website"
                android:autoLink="none"
                android:clickable="true"
                />

            <TextView
                android:text="https://twitter.com/"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:autoLink="none"
                android:clickable="true"
                android:id="@+id/link" />

            <TextView
                android:text="(323)123-1234"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:id="@+id/phoneNum"
                android:textSize="9pt"
                android:autoLink="phone"
                android:clickable="true"  />
        </TableRow>

    </TableLayout>


</TableRow>
